  "description": "APIS for trello clone web",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon ./server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
